<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>
  
  
  	<servlet>
	    <servlet-name>listResCon</servlet-name>
 		<display-name>listResCon</display-name>
	    <servlet-class>com.smhrd.controller.listResCon</servlet-class>
	</servlet>
	<servlet>
	    <servlet-name>ResultCon</servlet-name>
 		<display-name>ResultCon</display-name>
	    <servlet-class>com.smhrd.controller.ResultCon</servlet-class>
	</servlet>
	<servlet-mapping>
	    <servlet-name>listResCon</servlet-name>
	    <url-pattern>/listResCon</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
	    <servlet-name>ResultCon</servlet-name>
	    <url-pattern>/resultCon</url-pattern>
	</servlet-mapping>
</web-app>


